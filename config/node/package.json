{
  "name": "meta-assistant-ddf",
  "version": "1.0.0",
  "description": "Meta Software Engineering Platform",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "[ \"$SKIP_TYPE_CHECK\" = \"true\" ] && vite build || (tsc && vite build)",
    "build:prod": "NODE_ENV=production tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run \"tests/{unit,setup}/**/*.test.ts\" --root .",
    "test:integration": "vitest run \"tests/integration/**/*.test.ts\" --root .",
    "test:e2e": "vitest run \"tests/e2e/**/*.test.ts\" --root .",
    "cleanup:logs": "./scripts/cleanup_logs.sh",
    "test:logging": "vitest run server/src/__tests__/logging.test.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:strict": "eslint . --ext .ts,.tsx --max-warnings=0",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "ci": "npm run format:check && npm run lint:strict && npm run test:coverage",
    "release": "./scripts/release.sh",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint:strict && npm run type-check && npm run test && npm run sonar && npm run codeclimate",
    "fix": "npm run lint:fix && npm run format",
    "sonar": "sonar-scanner",
    "codeclimate": "codeclimate analyze",
    "quality": "npm run validate && npm run sonar && npm run codeclimate",
    "test:all": "./scripts/run_all_tests.sh",
    "typecheck": "tsc --noEmit",
    "fix:imports": "tsx scripts/fix-imports.ts",
    "fix:imports:dry-run": "tsx scripts/fix-imports.ts --dry-run"
  },
  "devDependencies": {
    "@playwright/test": "^1.51.1",
